import java.util.Scanner;

public class Atm {
	/*
	 * 아메리카노 1.1500원 카페라떼 2.2500원 스무디류 3.3000원
	 */

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		System.out.println("가진돈을 입력해주세요");
		int money = sc.nextInt();
		System.out.println("상품을 선택해 주세요");
	    int pay = sc.nextInt();
	    
	    switch (pay) {
		case 1:System.out.println("아메리카노");
			
			break;

		default:
			break;
		}
	    
	    
	}
}
