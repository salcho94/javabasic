package javaTest;

import java.util.Calendar;

public class First {

	public static void main(String[] args) {
		
		
		Calendar cal = Calendar.getInstance();
	       int year = 1994;
        int now = cal.get(Calendar.YEAR);
         int age = now-year;
         System.out.println(" age:"+age+","+" year:"+year);
         
		
	}

}
